diccionario ={ 
"NEstudiante": "" ,
"Nasignatura" : "" ,
"NoLab1": "" ,
"NoLab2" : "" ,
}
diccionario["nombre del estudiante"] = input("NEstudiante")
diccionario["Nombre asignatyra"] = input("Nasignatura")
diccionario["Nota lab 1"] = input("NoLab1")
diccionario["Nota lab 2"] = input("NoLab2")
Notafinal =float(diccionario["Nota laboratorio 1"]) *0.3 + float(diccionario["Nota lab 2"]) * 0.7

diccionario["promedio ponderado"] = f"{Notafinal:.1f}"

print(diccionario)